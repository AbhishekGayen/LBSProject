<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="com.infy.lbsprototype.model.PoiCategoryMsgIdMapping" table="poicategorymsgidmapping" >
        <id name="msgIdRef" type="java.lang.Long">
            <column name="msgIdRef" length="20" not-null="true" />
        </id>
        <property name="poiCategory" type="string">
            <column name="poiCategory" length="30" not-null="true" />
        </property>   
     </class>
</hibernate-mapping>
