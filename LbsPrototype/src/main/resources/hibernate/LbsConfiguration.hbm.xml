<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.infy.lbsprototype.model.LbsConfiguration" table="lbsconfiguration" >
  		<id name="confKey" type="string">
            <column name="confKey" length="30" not-null="true" />
        </id>
        <property name="confValue" type="string">
            <column name="confValue" length="30" not-null="true" />
        </property>      
     </class>
</hibernate-mapping>
